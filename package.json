{
  "name": "cryptofolio-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "firebase": "^8.4.1",
    "formik": "^2.2.6",
    "jest-dom": "^4.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-testing-library": "^8.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.23",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "react-scripts build && npx serve -s build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "semi": [
        "warn",
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "semi-style": [
        "warn",
        "last"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-multi-spaces": "warn",
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 1,
          "maxEOF": 0
        }
      ],
      "space-infix-ops": "warn",
      "comma-spacing": "warn",
      "block-spacing": "warn",
      "semi-spacing": "warn",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "key-spacing": "warn",
      "eol-last": "warn",
      "no-undef": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
